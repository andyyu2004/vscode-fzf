{
  "name": "vscode-fzf",
  "displayName": "vscode fzf",
  "publisher": "andyyu2004",
  "description": "",
  "version": "0.1.0",
  "repository": "https://github.com/andyyu2004/vscode-fzf",
  "engines": {
    "vscode": "^1.54.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:vscode-fzf.rg"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "configuration": {
      "properties": {
        "vscode-fzf.resultLimit": {
          "type": "number",
          "default": 100,
          "description": "The number of results before stopping"
        }
      }
    },
    "commands": [
      {
        "command": "vscode-fzf.rg",
        "title": "rg"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "test-compile": "tsc -p ./",
    "test-watch": "tsc -watch -p ./",
    "pretest": "yarn run test-compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.54.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0",
    "webpack": "^5.19.0",
    "webpack-cli": "^4.4.0"
  },
  "dependencies": {
    "@types/split2": "^2.1.6",
    "ripgrep-node": "^1.0.0",
    "split2": "^3.2.2",
    "vscode-ripgrep": "^1.11.2"
  }
}
